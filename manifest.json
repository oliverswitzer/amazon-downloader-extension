{
  "manifest_version": 3,
  "name": "Amazon Order Parser",
  "version": "1.0",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
